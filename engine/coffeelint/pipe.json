{
    "$schema": "https://repometric.github.io/linterhub/schema/extr.json",
    "id": "coffeelint",
    "name": "coffeelint",
    "pipeline": [
		{
			"cmd": "coffeelint",
			"success": 1,
			"engine" : true,
			"args": {
				"--reporter":"{{hub}}/{{engine}}/reporter.coffee"
			}
		}
	],
    "environment": {
		"executable": "coffeelint",
		"masks": true,
		"version": "v1.16.0"
    }
}