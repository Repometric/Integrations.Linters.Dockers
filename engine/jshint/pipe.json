{
    "$schema": "https://repometric.github.io/linterhub/schema/extr.json",
    "id": "jshint",
    "name": "jshint",
    "pipeline": [
		{
			"cmd": "jshint",
			"success": 2,
			"engine": true,
			"args": {
				"--reporter":"{{hub}}/{{engine}}/reporter.js"
			}
		}
	],
    "environment": {
		"executable": "jshint",
		"masks": true,
		"version": "v2.9.5"
    }
}